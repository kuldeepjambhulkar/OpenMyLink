{
    "manifest_version": 3,
    "name": "Open My Link",
    "version": "1.0",
    "description": "A Chrome extension for managing shortcuts in your web app.",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "action": {
      "default_icon": {
        "48": "logo.png"
      },
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["app.js"]
      }
    ]
  }
  